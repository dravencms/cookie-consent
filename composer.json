{
  "name": "dravencms/cookie-consent",
  "type": "dravencms-package",
  "description": "Cookie consent module for dravencms",
  "keywords": [
    "dravencms",
    "cookie-consent"
  ],
  "license": [
    "LGPL-3.0"
  ],
  "authors": [
    {
      "name": "Adam Schubert"
    }
  ],
  "require": {
    "dravencms/admin": ">=2.0"
  },
  "suggest": {
    "dravencms/structure": "Allows cookie consent components to be used in cms"
  },
  "require-dev": {
    "nette/tester": "@dev"
  },
  "autoload": {
    "psr-0": {
      "Dravencms\\": "src/"
    }
  },
  "extra": {
    "dravencms": {
      "files": {
        "assets/*": "%wwwDir%/assets/cookieConsent"
      },
      "configuration": "dravencms.config.neon"
    }
  }
}
